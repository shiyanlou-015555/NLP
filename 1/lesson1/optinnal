In [12]:
def pat_to_dict(patterns):
    return {k: v for k, v in patterns}
In [13]:
def subsitite(rule, parsed_rules):
    if not rule: return []

    return [parsed_rules.get(rule[0], rule[0])] + subsitite(rule[1:], parsed_rules)
